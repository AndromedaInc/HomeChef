{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "loose": true
      }
    ]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "dynamic-import-webpack",
  ],
  "env": {
    "server": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
